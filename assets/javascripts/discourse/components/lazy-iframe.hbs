{{#if @providerName}}
  <iframe
    src={{this.iframeSrc}}
    title={{@title}}
    allowFullScreen
    scrolling="no"
    frameborder="0"
    seamless="seamless"
    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
    referrerpolicy="strict-origin-when-cross-origin"
  ></iframe>
{{/if}}
